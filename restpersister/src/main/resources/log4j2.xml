<Configuration status="INFO" name="endava" packages="">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="WARN">  
            <AppenderRef ref="STDOUT"/>
        </Root>
        <Logger name="com.aqr.tca" level="ALL">
            <AppenderRef ref="Console"/>
        </Logger>
    </Loggers>
</Configuration>

